{
    "name": "lunkkun/caching-generator",
    "description": "A rewindable PHP Generator class that caches its generated values.",
    "keywords": [
        "php",
        "generator",
        "cache",
        "iterator"
    ],
    "homepage": "https://github.com/lunkkun/php-caching-generator",
    "license": "MIT",
    "authors": [
        {
            "name": "Luuk Linders",
            "email": "lunkkun@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Lunkkun\\CachingGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lunkkun\\CachingGenerator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
